import java.util.ArrayList;
import java.util.List;

public class Principal {
	@FunctionalInterface
	public interface Inteiro {
		int getValue();
	}

	public static void main(String[] args) {

		Inteiro i = () -> 42;
		System.out.println(i.getValue());
		i = () -> 49;
		System.out.println(i.getValue());
	}
